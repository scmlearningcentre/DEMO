pipeline {
    agent any
    stages {
        stage('Compile Stage') {
		  steps {
		      withMaven(maven : 'maven') {
                 sh 'mvn clean compile'
				 }
				}
			  }
		stage('testing Stage') {
		  steps {
		      withMaven(maven : 'maven') {
                 sh 'mvn test'
				 }
				}
			  }
		stage('Deploy Stage') {
		  steps {
		      withMaven(maven : 'maven') {
                 sh 'mvn deploy'
				 }
			}
		}
	}
}
